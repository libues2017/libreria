	<parameter name="par_editorial" class="java.lang.Integer"/>
	<parameter name="par_consig" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT libues.producto.`CODIGOPRODUCTO`,
	libues.producto.`CODIGOPROVEEDOR`,
	libues.producto.`CODIGOAREA`,
	libues.producto.`CODTIPOPRODUCTO`,
	libues.producto.`NOMBREPRODUCTO`,
	libues.producto.`CODIGOEDITORIAL`,
	libues.producto.`PRECIO`,
	libues.producto.`CONSIGNACION`,
	libues.autor.`NOMBREAUTOR`,
	libues.autor.`CODIGOAUTOR`,
	libues.producto.`EXISTENCIA`,
	libues.tipoproducto.`TIPOPRODUCTO`,
	libues.proveedor.`NOMBREPROVEEDOR`,
	libues.area.`NOMBREAREA`
FROM libues.autor_libro
	INNER JOIN libues.producto ON 
	 libues.autor_libro.`CODIGOPRODUCTO` = libues.producto.`CODIGOPRODUCTO` 
	INNER JOIN libues.proveedor ON 
	 libues.producto.`CODIGOPROVEEDOR` = libues.proveedor.`CODIGOPROVEEDOR` 
	INNER JOIN libues.area ON 
	 libues.producto.`CODIGOAREA` = libues.area.`CODIGOAREA` 
	INNER JOIN libues.tipoproducto ON 
	 libues.producto.`CODTIPOPRODUCTO` = libues.tipoproducto.`CODTIPOPRODUCTO` 
	INNER JOIN libues.editorial ON 
	 libues.producto.`CODIGOEDITORIAL` = libues.editorial.`CODIGOEDITORIAL` 
	INNER JOIN libues.autor ON 
	 libues.autor_libro.`CODIGOAUTOR` = libues.autor.`CODIGOAUTOR`
	 WHERE  
	( libues.producto.`CODIGOAREA`= $P{par_area} ) AND
	(libues.producto.`CODIGOEDITORIAL`= $P{par_editorial} ) AND
	(libues.producto.`CODTIPOPRODUCTO` = $P{par_tipoprod}  ) AND
	(libues.producto.`CODIGOPROVEEDOR`= $P{par_provee} ) AND
	(libues.producto.`CONSIGNACION`= $P{par_consig} )]]>
	</queryString>
